  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 24rpx;
}
.van-popup .edit .left image {
  width: 32rpx;
  height: 32rpx;
}
.van-popup .edit .left text {
  margin-left: 32rpx;
}
.van-popup .edit .right {
  color: #BFBFBF;
}
.van-popup .remove {
  display: flex;
