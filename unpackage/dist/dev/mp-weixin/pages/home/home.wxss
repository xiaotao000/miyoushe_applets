/* 搜索 */
.search {
  height: 120rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
  width: 100%;
  background-image: url("data:image/jpeg;base64,");
  background-repeat: no-repeat;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search image {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
}
.search .serch-right {
  width: 90%;
  height: 84rpx;
  margin-left: 24rpx;
  background-color: #111;
  opacity: 0.5;
  display: flex;
  color: #fff;
  padding: 0 24rpx;
  justify-content: center;
  border-radius: 24rpx;
}
.search .serch-right .serch-input {
  padding-left: 24rpx;
  width: 90%;
  height: 100%;
}

